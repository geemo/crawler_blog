{
    "appid": "crawler_github",
    "author": "zhongyang.zhou@ele.me",
    "notice": ["zhongyang.zhou@ele.me"],
    "services": [{
        "serviceName": "crawler_github",
        "funcs": [{
            "functionName": "user",
            "handler": "es5/user.handler",
            "memorySize": 128,
            "runtime": "nodejs4.4",
            "timeout": 300
        }, {
            "functionName": "repos",
            "handler": "es5/repos.handler",
            "memorySize": 128,
            "runtime": "nodejs4.4",
            "timeout": 300
        }, {
            "functionName": "blog_analy",
            "handler": "es5/blog_analy.handler",
            "memorySize": 1000,
            "runtime": "nodejs4.4",
            "timeout": 300
        }]
    }],
    "build": {
      "commands": [
        "npm i",
        "npm run build-es5"
      ]
    }
}
