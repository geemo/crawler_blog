{
    "appid": "crawler_github",
    "author": "zhongyang.zhou@ele.me",
    "notice": ["zhongyang.zhou@ele.me"],
    "services": [{
        "serviceName": "crawler_github",
        "funcs": [{
            "functionName": "user",
            "handler": "es5/user.handler",
            "memorySize": 128,
            "runtime": "nodejs4.4",
            "timeout": 300
        }, {
            "functionName": "repo",
            "handler": "es5/repo.handler",
            "memorySize": 128,
            "runtime": "nodejs4.4",
            "timeout": 300
        }]
    }],
    "build": {
      "commands": [
        "npm i",
        "npm run build-es5"
      ]
    },
    "deploy": {
        "env": {
            "alpha": {
                "TOKEN": ""
            },
            "beta": {
                "TOKEN": ""
            },
            "production": {
                "TOKEN": ""
            }
        }
    }
}
